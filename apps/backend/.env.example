# AI Agent Team Backend - Environment Variables

# ===== Server Configuration =====
PORT=8000
ENVIRONMENT=development
BACKEND_URL=http://localhost:8000
ALLOWED_ORIGINS=https://my-ai-agent-team-seven.vercel.app,http://localhost:3000,http://localhost:19006

# ===== Database (Supabase) =====
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_service_role_key

# ===== Redis/Upstash =====
REDIS_URL=your_redis_url

# ===== OpenRouter API =====
OPENROUTER_API_KEY=your_openrouter_api_key
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# ===== AI Models =====
PRODUCT_MANAGER_MODEL=nvidia/nemotron-4-340b-instruct
FINANCE_MANAGER_MODEL=nvidia/nemotron-4-340b-instruct
MARKETING_STRATEGIST_MODEL=nvidia/nemotron-4-340b-instruct
LEADGEN_MODEL=anthropic/claude-3-haiku
EMAILER_MODEL=anthropic/claude-3-haiku
CALLPREP_MODEL=anthropic/claude-3-haiku
ENGINEER_MODEL=anthropic/claude-3-haiku

# ===== Gmail API =====
GMAIL_CREDENTIALS_JSON={"type":"service_account","project_id":"..."}

# ===== Security =====
INTERNAL_SIGNING_KEY=your_internal_jwt_key_min_32_chars
WEBHOOK_SECRET=your_webhook_hmac_secret_min_32_chars

# ===== Rate Limiting & Caching =====
MODEL_CALL_TIMEOUT=20
SCRAPE_TIMEOUT=15
MAX_CONCURRENT_SCRAPES=5
MAX_CONCURRENT_EMAILS=10
SCRAPE_CACHE_TTL=86400
MODEL_CACHE_TTL=86400

# ===== Monitoring (Optional) =====
SENTRY_DSN=your_sentry_dsn
PROMETHEUS_PORT=9090
